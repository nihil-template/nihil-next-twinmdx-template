{
  "name": "nihil-next-twin-template",
  "version": "1.0.0",
  "description": "Nextjs 편하게 쓰려고 만든 템플릿. 타입스크립트 기본 지원. Emotion & TailwindCSS 지원",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nihil-template/nihil-next-twin-template.git"
  },
  "bugs": {
    "url": "https://github.com/nihil-template/nihil-next-twin-template/issues"
  },
  "homepage": "https://github.com/nihil-template/nihil-next-twin-template#readme",
  "scripts": {
    "remove": "rm -rf node_modules/.cache && rm -rf build/",
    "serve": "yarn run remove && next dev",
    "start": "yarn run remove && next build && next start",
    "build": "yarn run remove && next build",
    "backupgit": "mv ../io/.git ../backup",
    "restoregit": "mv ../backup/.git ../io",
    "export": "yarn run backupgit && yarn run remove && next build && next export -o ../io/ && touch ../io/.nojekyll && yarn run restoregit && cd ../io && git add .",
    "clean": "yarn cache clean"
  },
  "keywords": [],
  "author": "NIHILncunia",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@reduxjs/toolkit": "^1.8.3",
    "gray-matter": "^4.0.3",
    "next": "^12.2.4",
    "next-mdx-remote": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "redux": "^4.2.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@emotion/babel-plugin": "^11.10.0",
    "@emotion/babel-preset-css-prop": "^11.10.0",
    "@types/node": "^18.6.4",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-redux": "^7.1.24",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "autoprefixer": "^10.4.8",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-twin": "^1.0.2",
    "eslint": "^8.21.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^12.2.4",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.7",
    "twin.macro": "^3.0.0-rc.3",
    "typescript": "^4.7.4"
  },
  "babelMacros": {
    "twin": {
      "preset": "emotion"
    }
  }
}
