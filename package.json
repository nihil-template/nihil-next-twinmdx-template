{
  "name": "nihil-next-twin-template",
  "version": "1.0.0",
  "description": "Nextjs 편하게 쓰려고 만든 템플릿. 타입스크립트 기본 지원. Emotion & TailwindCSS 지원",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nihil-template/nihil-next-twin-template.git"
  },
  "bugs": {
    "url": "https://github.com/nihil-template/nihil-next-twin-template/issues"
  },
  "homepage": "https://github.com/nihil-template/nihil-next-twin-template#readme",
  "scripts": {
    "remove": "rm -rf node_modules/.cache && rm -rf build/",
    "serve": "yarn run remove && next dev",
    "start": "yarn run remove && next build && next start",
    "build": "yarn run remove && next build",
    "backupgit": "mv ../io/.git ../backup",
    "restoregit": "mv ../backup/.git ../io",
    "export": "yarn run backupgit && yarn run remove && next build && next export -o ../io/ && touch ../io/.nojekyll && yarn run restoregit && cd ../io && git add .",
    "clean": "yarn cache clean",
    "open": "start http://localhost:3000"
  },
  "keywords": [],
  "author": "NIHILncunia",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@reduxjs/toolkit": "^1.8.4",
    "gray-matter": "^4.0.3",
    "moment": "^2.29.4",
    "next": "^12.2.5",
    "next-mdx-remote": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-redux": "^8.0.2",
    "redux": "^4.2.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@emotion/babel-plugin": "^11.10.0",
    "@emotion/babel-preset-css-prop": "^11.10.0",
    "@svgr/webpack": "^6.3.1",
    "@types/node": "^18.7.6",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-redux": "^7.1.24",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "autoprefixer": "^10.4.8",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-twin": "^1.0.2",
    "eslint": "^8.22.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^12.2.5",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.16",
    "tailwindcss": "^3.1.8",
    "twin.macro": "^3.0.0-rc.3",
    "typescript": "^4.7.4"
  },
  "babelMacros": {
    "twin": {
      "preset": "emotion"
    }
  }
}
